{
    "manifest_version": 2,
    "name": "Link Sanitizer",
    "version": "0.1",
  
    "description": "Remove source identifiers on URLs.",
    "permissions": ["webRequest", "webRequestBlocking", "<all_urls>"],
    "background": {
      "scripts": ["js/listener.js"]
    },
  
    "icons": {
      "48": "icons/border-48.png",
      "96": "icons/border-96.png"
    }
  }
  